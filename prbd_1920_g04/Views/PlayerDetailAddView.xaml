        <TextBox Text="{Binding Heigth}" HorizontalAlignment="Left" Height="23" Margin="146,312,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="198"/>
        <TextBox Text="{Binding Password}" HorizontalAlignment="Left" Height="23" Margin="459,312,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="195"/>
        <TextBox Text="{Binding JerseyNumber}" HorizontalAlignment="Left" Height="23" Margin="459,267,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="195"/>
        <TextBox Text="{Binding Weight}" HorizontalAlignment="Left" Height="23" Margin="459,222,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="195"/>
        <Label Content="Firstname :" HorizontalAlignment="Left" Margin="52,86,0,0" VerticalAlignment="Top"/>
        <Label Content="LastName : " HorizontalAlignment="Left" Margin="52,130,0,0" VerticalAlignment="Top"/>
        <Label Content="Age :" HorizontalAlignment="Left" Margin="52,175,0,0" VerticalAlignment="Top"/>
        <Label Content="Email : " HorizontalAlignment="Left" Margin="52,219,0,0" VerticalAlignment="Top"/>
        <Label Content="Adresse :" HorizontalAlignment="Left" Margin="52,264,0,0" VerticalAlignment="Top"/>
        <Label Content="Height :" HorizontalAlignment="Left" Margin="52,309,0,0" VerticalAlignment="Top"/>
        <Label Content="JerseyNumber :" HorizontalAlignment="Left" Margin="359,264,0,0" VerticalAlignment="Top"/>
        <Label Content="Weight :" HorizontalAlignment="Left" Margin="388,219,0,0" VerticalAlignment="Top"/>
        <Label Content="Password :" HorizontalAlignment="Left" Margin="377,312,0,0" VerticalAlignment="Top"/>
        <Image HorizontalAlignment="Left" Height="100" Margin="511,86,0,0" VerticalAlignment="Top" Width="100"/>
        <Button Command="{Binding Cancel}" Content="Cancel" HorizontalAlignment="Left" Margin="297,387,0,0" VerticalAlignment="Top" Width="75"/>
        <Button Command="{Binding Save}" Content="Save" HorizontalAlignment="Left" Margin="439,387,0,0" VerticalAlignment="Top" Width="75"/>
